{
  "compilerOptions": {
    "allowJs": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "strict": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2015", "dom"],
    "outDir": "dist",
    "module": "es2015"
  },
  "exclude": ["node_modules", "wwwroot", "dist", "webpack.config.js"]
}
