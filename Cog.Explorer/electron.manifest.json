{
  "executable": "SIL.Cog.Explorer",
  "splashscreen": {
    "imageFile": "/wwwroot/splash-screen.png"
  },
  "singleInstance": false,
  "build": {
    "appId": "com.SIL.Cog.Explorer.app",
    "productName": "Cog Explorer",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "icon": "../../../Assets/cog.png"
    },
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}